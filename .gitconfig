[diff]
external    = vimdiff-git.sh

[pager]
diff        =

[user]
	email = twolights@gmail.com
	name = Yi-Kuang Chen
[core]
	quotepath = false
	excludesfile = /Users/ykchen/.gitignore_global
	ignorecase = false

[alias]
    a       = add
    br      = branch
    co      = checkout
    dif     = diff
    l       = log
    mt      = mergetool
    st      = status
    stat    = status

[merge]
    tool    = vimdiff
    conflictstyle = diff3
